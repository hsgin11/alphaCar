<script type="text/javascript">
	var optionList = 1;
	var $select = $('#option');
	$select.change(function() {
		
		if($select.val() != 'cho'){
			var idx = $("#option option").index($('#option option:selected'));
			var optionPrice = ${prod.prodPrice } + $(this).val() * 1;
			var str = '<div class="row data">';
			str +='<div class="subdiv">';
			str +='<div class="check"><input type="checkbox" name="buy" value="260" checked="" onclick="javascript:basket.checkItem();">&nbsp;</div>';
			str +='<div class="pname">';
			str +='<span>${prod.prodName } 옵션 :'+$('#option option:selected').text()+' </span>';
			str +='</div></div>';
			str +=' <div class="subdiv">';
			str +='<div class="basketprice"><input type="hidden" name="orders[0].options['+(optionList-1)+'].optionPrice" id="p_price'+optionList+'" class="p_price" value="'+optionPrice+'"><input type="hidden" class="optionNo" name="orders[0].options['+(optionList-1)+'].optionNo" value="'+idx+'">'+optionPrice+'</div>';
			str +='<div class="num">';
			str +=' <div class="updown">';
			str +='<input type="text" name="orders[0].options['+(optionList-1)+'].prodQty" id="p_num'+optionList+'" size="2" maxlength="4" class="p_num" value="1" onkeyup="javascript:basket.changePNum('+optionList+');">';
			str +='<div class="d-flex justify-content-end">';
			str +=' <span onclick="javascript:basket.changePNum('+optionList+');"><i class="bi bi-plus-circle up"></i></span>';
			str +=' <span onclick="javascript:basket.changePNum('+optionList+');"><i class="bi bi-dash-circle down"></i></span>';
			str +='</div></div></div>';
			str +='<div class="sum">'+optionPrice+'</div></div>';
			str +='<div class="subdiv">';
			str +='<div class="basketcmd"><a href="javascript:void(0)" class="abutton" onclick="javascript:basket.delItem();">삭제</a></div>';
			str +='</div></div>';
	 	    
			$('#basket').append(str)
			var count = 0;
			$('.p_num').each(function(i, element) {
				count += element.value * 1;
			})
			var sum_price=0;
			$('.sum').each(function(i, element) {
				sum_price += element.innerHTML.replace(/,/gi, '') * 1;
			})
			$('input[name=sumPrice]').val(sum_price);
			sum_price += '';
			console.log(sum_price);
			if(sum_price==0){ 
				sum_price = 0;
			}else{
		    	var regex = /(^[+-]?\d+)(\d{3})/;
		    	var nstr = (sum_price + '');
		    	while (regex.test(sum_price)) {
		    		sum_price = sum_price.replace(regex, '$1' + ',' + '$2');
		    	}
			}
			$('#sum_p_num').text('상품갯수: ' + count+'개');
			$('#sum_p_price').text('합계금액: '+sum_price+'원');
			
			optionList = optionList + 1;
			
		}
	})
	$('#add-basket').click(function() {
		
		var $options = $('.row.data');
		var str_options_no = [];
		var str_options_qty = [];
		var str_options_price = [];
		$options.each(function(i, element) {
			str_options_no.push($(element).find('.optionNo').val());
			str_options_qty.push($(element).find('.p_num').val());
			str_options_price.push($(element).find('.p_price').val());
		});
		var basketItem = {"prodNo" : "${prod.prodNo}","userId" : "${sessionScope.USER_INFO.userId}","prodSeller" : "${prod.prodSeller}","prodName" : "${prod.prodName}","optionNo" : str_options_no,"optionPrice" : str_options_price ,"prodQty" : str_options_qty};
		sessionStorage.setItem("${prod.prodNo}",JSON.stringify(basketItem));
		swal("장바구니에 담겼습니다.", "", "success");
		
	})
	function get_comment(curPage) {
		var params = {"prodNo" : "${prod.prodNo}", "curPage" : curPage}
		$.ajax({
			 type : 'POST'
			,url : '<c:url value="/comment" />'
		   	,data : params
		   	,dataType:'JSON'
		   	,success: function(data) {
		   		console.log(data);
				var str = '';
				$div=$('#comment');
				$.each(data.data, function(index, element) {
					str += '<tr height="50px">';
					str +='<td width="100px"><strong>'+element.userId+'</strong></td>';
					str +='<td>';
					str += '<span>'+element.prodComment+'</span></td>';
					str +='</tr>';
				});
				$div.html(str)
				var page = '<nav aria-label="Page navigation example" class="d-flex justify-content-center">'
				page += '<ul class="pagination1">';
				page += '<li class="page-item"><a class="page-link" href="javascript:get_comment(1)"><span aria-hidden="true">&laquo;</span></a></li>';
				if(data.searchVO.firstPage != 1){
					page += '<li class="page-item">';
					page += '<a class="page-link" href="javascript:get_comment(data.searchVO.firstPage - 1)'+'" aria-label="Previous">';
					page += '<span aria-hidden="true">&lt;</span>';
					page += '</a>';
					page += '</li>';
				}
				for(var i = data.searchVO.firstPage; i <= data.searchVO.lastPage; i++){
					if(data.searchVO.curPage != i){
						page += '<li class="page-item"><a class="pagination1" href="javascript:get_comment('+i+')">'+i+'</a></li>';
					}
					if(data.searchVO.curPage == i){
						page += '<li class="page-item active" aria-current="page"><a class="pagination1 active" href="#">'+i+'</a></li>';
					}
				}
				if(data.searchVO.lastPage != data.searchVO.totalPageCount){
					page += '<li class="page-item">';
					page += '<a class="page-link" href="javascript:get_comment(searchVO.lastPage + 1)"aria-label="Next">';
					page += '<span aria-hidden="true">&gt;</span>';
					page += '</a>';
					page += '</li>';
				}
				page += '<li class="page-item"><a class="page-link" href="javascript:get_comment(searchVO.totalPageCount)"><span aria-hidden="true">&raquo;</span></a></li>';
				page += '</ul>';
				page += '</nav>';
				$('#comment-paging').html('');
				$('#comment-paging').append(page); 			
		    }
   		});
	}
	get_comment(1);
</script>


